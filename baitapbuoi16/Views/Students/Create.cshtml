@model baitapbuoi16.DataAccess.DTO.ReturnDataReturnStudent

<h2>Thêm học sinh mới</h2>

<form asp-action="Create">
    <div class="form-group">
        <label for="StudentName">Tên:</label>
        <input type="text" class="form-control" id="StudentName" name="StudentName" required>
    </div>
    <div class="form-group">
        <label for="StudentClass">Lớp:</label>
        <input type="text" class="form-control" id="StudentClass" name="StudentClass" required>
    </div>
    <div class="form-group">
        <label for="StudentGender">Giới tính:</label>
        <input type="text" class="form-control" id="StudentGender" name="StudentGender" required>
    </div>
    <div class="form-group">
        <label for="StudentAge">Tuổi:</label>
        <input type="text" class="form-control" id="StudentAge" name="StudentAge"  required>
    </div>
    <div class="form-group">
        <label for="StudentID">ID:</label>
        <input type="text" class="form-control" id="StudentID" name="StudentID" required>
    </div>
    <div class="form-group">
        <label for="StudentEmail">Email:</label>
        <input type="text" class="form-control" id="StudentEmail" name="StudentEmail"required>
    </div>
    <button type="submit" class="btn btn-primary">Thêm</button>
   
</form>